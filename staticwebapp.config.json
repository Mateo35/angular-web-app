{
    "trailingSlash": "never",
    "routes": [
        {
            "route": "/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif,ico}",
            "/*.{css,scss,js}"
        ]
    }
}